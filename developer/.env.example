# ============================================
# SECURITY
# ============================================
# Secret for signing JWT tokens
JWT_SECRET=your-super-secret-jwt-key-min-32-chars

# Secret for encrypting sensitive data
ENCRYPTION_KEY=your-encryption-key-min-32-chars

# API Key encryption salt
API_KEY_SALT=your-api-key-salt-min-16-chars

# ============================================
# OAUTH CONFIGURATION
# ============================================
# Developer Portal OAuth Client (for testing)
DEV_PORTAL_CLIENT_ID=dev-portal-client
DEV_PORTAL_CLIENT_SECRET=dev-portal-secret-key

# CatalystWells OAuth endpoints (Main App)
CATALYSTWELLS_OAUTH_AUTHORIZE_URL=https://app.catalystwells.com/authsso
CATALYSTWELLS_OAUTH_TOKEN_URL=https://app.catalystwells.com/api/oauth/token
CATALYSTWELLS_OAUTH_USERINFO_URL=https://app.catalystwells.com/api/oauth/userinfo

# ============================================
# APPLICATION SETTINGS
# ============================================
# Base URL for the developer portal
NEXT_PUBLIC_DEV_PORTAL_URL=https://developers.catalystwells.com
NEXT_PUBLIC_CATALYSTWELLS_URL=https://app.catalystwells.com

# Environment
NODE_ENV=production

# ============================================
# EMAIL CONFIGURATION (for notifications)
# ============================================
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_FROM=noreply@catalystwells.com

# ============================================
# RATE LIMITING
# ============================================
# Redis URL for rate limiting (optional, uses in-memory if not set)
REDIS_URL=redis://localhost:6379

# Default rate limits
DEFAULT_RATE_LIMIT_PER_MINUTE=60
DEFAULT_RATE_LIMIT_PER_HOUR=1000
DEFAULT_RATE_LIMIT_PER_DAY=10000

# ============================================
# WEBHOOKS
# ============================================
# Webhook delivery timeout (ms)
WEBHOOK_TIMEOUT=5000

# Max retry attempts for failed webhooks
WEBHOOK_MAX_RETRIES=3

# ============================================
# FILE UPLOADS
# ============================================
# Max file size for logos/banners (in bytes)
MAX_LOGO_SIZE=2097152  # 2MB
MAX_BANNER_SIZE=5242880  # 5MB

# Allowed file types
ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp,image/svg+xml

# ============================================
# ANALYTICS
# ============================================
# Enable analytics tracking
ENABLE_ANALYTICS=true

# Analytics retention period (days)
ANALYTICS_RETENTION_DAYS=90

# ============================================
# ADMIN
# ============================================
# Admin email addresses (comma-separated)
ADMIN_EMAILS=admin@catalystwells.com,support@catalystwells.com

# ============================================
# FEATURE FLAGS
# ============================================
ENABLE_WEBHOOKS=true
ENABLE_API_KEYS=true
ENABLE_ANALYTICS_EXPORT=true
ENABLE_TWO_FACTOR_AUTH=true

# ============================================
# MONITORING
# ============================================
# Sentry DSN for error tracking (optional)
SENTRY_DSN=

# Log level
LOG_LEVEL=info  # debug, info, warn, error
